{
  "name": "@pdir/vue-trustedshops",
  "version": "0.1.7",
  "description": "Vue plugin for Trustedshops",
  "keywords": [
    "vue",
    "components",
    "trustedshops",
    "reviews",
    "testimonial"
  ],
  "author": "Mathias Arzberger (https://github.com/pdir)",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-bundle": "vue-cli-service build --target lib --name vue-trustedshops ./src/components/index.js",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "vue": "^2.5.17",
    "axios": "^0.18.0",
    "vue-carousel": "^0.6.9"
  },
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js"
  ],
  "main": "./dist/vue-trustedshops.common.js",
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.4",
    "@vue/cli-plugin-eslint": "^3.0.4",
    "@vue/cli-service": "^3.0.4",
    "vue-template-compiler": "^2.5.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/pdir/vue-trustedshops.git"
  },
  "licenses": [
    {
      "type": "GPL-3.0-or-later"
    }
  ]
}
